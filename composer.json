{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Upload\\": "vendor/codeguy/upload/src",
            "Tests\\": "tests",
            "App\\Command\\": "src/commands"
        }
    },
    "require-dev": {
        "symfony/debug": "^4",
        "phpunit/phpunit": "^5",
        "phpunit/php-invoker": "^2",
        "phpunit/php-code-coverage": "4",
        "symfony/browser-kit": "^5",
        "symfony/event-dispatcher": "^5",
        "symfony/filesystem": "^5",
        "symfony/process": "^5",
        "there4/slim-test-helpers": "^2",
        "symfony/flex": "^1"
    },
    "require": {
        "php": "^7.3",
        "slim/slim": "~3.1",
        "slim/twig-view": "^2",
        "slim/flash": "^0.4",
        "monolog/monolog": "^2",
        "codeguy/upload": "^1.3",
        "illuminate/database": "^6",
        "ircmaxell/password-compat": "^1",
        "phpoffice/phpspreadsheet": "^1",
        "symfony/console": "^5",
        "ext-json": "^1.6",
        "ext-PDO": "^7.2",
        "symfony/dom-crawler": "^5",
        "ext-curl": "^7.3",
        "symfony/css-selector": "^5",
        "joshfraser/php-name-parser": "dev-master",
        "components/bootstrap-default": "^3.3"
    }
}
