{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Upload\\": "vendor/codeguy/upload/src",
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "symfony/debug": "^3.1",
        "phpunit/phpunit": "^5.6",
        "phpunit/php-invoker": "^1.1",
        "phpunit/php-code-coverage": "^4.0",
        "components/bootstrap-default": "^3.3",
        "silex/silex": "^2.0",
        "symfony/browser-kit": "^3.1",
        "phpunit/dbunit": "^2.0",
        "there4/slim-test-helpers": "^2.1",
        "symfony/console": "^3.2",
        "symfony/event-dispatcher": "^3.2",
        "symfony/filesystem": "^3.2",
        "symfony/process": "^3.2"
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "~1.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "codeguy/upload": "^1.3",
        "flexsounds/slim-symfony-di-container": "^1.0",
        "illuminate/database": "~5.1",
        "ircmaxell/password-compat": "^1.0",
        "phpoffice/phpspreadsheet": "^1.3"
    }
}
