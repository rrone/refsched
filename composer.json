{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Upload\\": "vendor/codeguy/upload/src"
        }
    },
    "require-dev": {
        "symfony/debug": "^3.1",
        "phpunit/phpunit": "^5.6"
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "codeguy/upload": "^1.3",
        "phpoffice/phpexcel": "^1.8",
        "flexsounds/slim-symfony-di-container": "^1.0",
        "illuminate/database": "~5.1",
        "phpoffice/phpExcel": "~1.8.1",
        "ircmaxell/password-compat": "^1.0",
        "tuupola/slim-jwt-auth": "^2.3",
        "vlucas/phpdotenv": "^2.4",
        "firebase/php-jwt": "^4.0"
    }
}
