{
    "name": "slim3/refsched",
    "description": "Slim Framework 3 application to manage referee schedules.",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Upload\\": "vendor/codeguy/upload/src",
            "Tests\\": "tests",
            "App\\Command\\": "src/commands"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "symfony/debug": "^4",
        "phpunit/phpunit": "^6",
        "phpunit/php-invoker": "^2",
        "phpunit/php-code-coverage": "^5",
        "symfony/browser-kit": "^5",
        "symfony/event-dispatcher": "^5",
        "symfony/filesystem": "^5",
        "symfony/process": "^5",
        "there4/slim-test-helpers": "^2",
        "symfony/flex": "^1"
    },
    "require": {
        "php": "^7.3",
        "slim/slim": "^3",
        "slim/twig-view": "^2",
        "slim/flash": "^0.4",
        "monolog/monolog": "^2",
        "codeguy/upload": "^1.3",
        "illuminate/database": "^6",
        "ircmaxell/password-compat": "^1",
        "phpoffice/phpspreadsheet": "^1",
        "symfony/console": "^5",
        "ext-PDO": "^7.2",
        "symfony/dom-crawler": "^5",
        "ext-curl": "^7.3",
        "symfony/css-selector": "^5",
        "joshfraser/php-name-parser": "dev-master",
        "components/bootstrap-default": "^3.3",
        "symfony/dotenv": "^5.0",
        "tamtamchik/namecase": "^2.1",
        "ext-json": "^1"
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --testdox",
        "demo": "php examples/demo.php"
    }
}
